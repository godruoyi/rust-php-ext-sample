{
    "name": "godruoyi/rust-php-extension-sample",
    "description": "Installation script for sample rust php extension",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "godruoyi",
            "email": "godruoyi@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*"
    },
    "require-dev": {
        "pestphp/pest": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Godruoyi\\Extension\\": [
                "extension/"
            ]
        }
    },
    "bin": [
        "bin/install-sample-extension"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
